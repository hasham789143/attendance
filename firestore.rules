
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and grant open access to your entire database.
    // Any user can read, write, and delete any data.
    // This is not recommended for production applications.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
